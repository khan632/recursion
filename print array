package com.company;

import java.util.Scanner;

public class Main {
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int [] arr = {2, 3, 4, 5, 11, 111};
        printArr(arr, 0);
    }

    public static void printArr(int [] arr, int idx){
        if(idx == arr.length){
            return;
        }
        System.out.println(arr[idx]);
        printArr(arr, idx+1);
    }
  }
